{
  "extends": "airbnb-base",
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "semi": 0,
    "no-plusplus": 0,
    "class-methods-use-this": 0,
    "no-useless-escape": 0,
    "no-underscore-dangle": 0,
    "template-curly-spacing": 0,
    "prefer-template": 0,
    "no-continue": 0,
    "prefer-destructuring": 0,
    "no-cond-assign": 0
  },
  "env": {
    "es6": true,
    "node": true,
    "browser": true,
    "jasmine": true
  }
}
